{
  "name": "neonami",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/config/dataSource.ts",
    "migration:generate": "npm run typeorm -- migration:generate src/migrations/UpdateProductCategoryOnDelete",
    "migration:create": "npm run node --require ts-node/register --require tsconfig-paths/register ./node_modules/typeorm/cli.js -- migration:create src/migrations/AddViews",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.8",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^8.0.5",
    "@nestjs/typeorm": "^10.0.2",
    "@types/multer": "^1.4.12",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "archiver": "^7.0.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-parse": "^5.5.6",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.5",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "prom-client": "^15.1.3",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/node": "^22.8.6",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^@entities/(.*)$": "<rootDir>/entities/$1",
      "^@dto/(.*)$": "<rootDir>/dto/$1",
      "^@services/(.*)$": "<rootDir>/services/$1"
    }
  }
}
