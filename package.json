{
  "name": "neonami",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:prod": "ts-node -r tsconfig-paths/register dist/main.js",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/config/dataSource.ts",
    "migration:run:ts": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run -d src/config/dataSource.ts",
    "migration:run": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run -d dist/config/dataSource.js",
    "migration:revert": "npm run typeorm migration:revert -d src/config/dataSource.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@influxdata/influxdb-client": "^1.35.0",
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.8",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^8.0.5",
    "@nestjs/typeorm": "^10.0.2",
    "@types/multer": "^1.4.12",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "archiver": "^7.0.1",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-parse": "^5.5.6",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.5",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "prom-client": "^15.1.3",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/node": "^22.8.6",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^@entities/(.*)$": "<rootDir>/entities/$1",
      "^@dto/(.*)$": "<rootDir>/dto/$1",
      "^@services/(.*)$": "<rootDir>/services/$1"
    }
  },
  "_moduleAliases": {
    "@services": "./dist/services",
    "@entities": "./dist/entities",
    "@dto": "./dist/dto",
    "@controllers": "./dist/controllers"
  }
}
